{
  "name": "decentraland-gatsby",
  "version": "0.0.0-development",
  "description": "Common Gastby components",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12.0.0",
    "npm": "^6.0.0"
  },
  "bin": {
    "setup-project": "./bin/setup-project.sh",
    "setup-global-css": "./bin/setup-global-css.sh"
  },
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && tsc -p . -outDir dist --sourceMap false --skipLibCheck && postcss --base src --dir dist 'src/**/*.css'",
    "start": "npm run storybook",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,mdx,css}'",
    "storybook": "start-storybook -s static -p 6006",
    "semantic-release": "semantic-release",
    "build-storybook": "build-storybook --quiet && cp ./static/* ./storybook-static && mv ./storybook-static ./public",
    "husky-setup": "husky install",
    "migrate": "DOTENV_CONFIG_PATH=.env.development ts-node -r dotenv/config ./node_modules/node-pg-migrate/bin/node-pg-migrate -j ts -m src/migrations -d CONNECTION_STRING",
    "precommit": "lint-staged --config .husky/pre-commit.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/decentraland-growth-ui.git"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "setupFiles": [
      "<rootDir>/src/storyshots.setup.ts"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "./jest.transform.js",
      "^.+\\.mdx?$": "@storybook/addon-docs/jest-transform-mdx"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|gif|ttf|eot|svg)$": "<rootDir>/src/__mocks__/files.ts"
    }
  },
  "author": "",
  "license": "MIT",
  "release": {
    "ci": false
  },
  "bugs": {
    "url": "https://github.com/decentraland/decentraland-growth-ui/issues"
  },
  "homepage": "https://github.com/decentraland/decentraland-growth-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime-corejs3": "^7.12.13",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-docs": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.3.10",
    "@storybook/addon-storysource": "^6.1.18",
    "@storybook/addon-viewport": "^6.1.18",
    "@storybook/addons": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@types/flat": "0.0.28",
    "@types/html-escaper": "^3.0.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^25.1.3",
    "@types/lodash.omit": "^4.5.6",
    "@types/node": "^12.19.7",
    "@types/react-dom": "^16.9.5",
    "@types/react-highlight": "^0.12.2",
    "@types/segment-analytics": "0.0.32",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.3",
    "flat": "^5.0.0",
    "jest": "^25.1.0",
    "postcss": "^8.3.11",
    "postcss-assets": "^6.0.0",
    "postcss-svg": "^3.0.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-test-renderer": "^17.0.2",
    "semantic-release": "^17.4.7",
    "sql-formatter": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "@dcl/feature-flags": "^1.0.0",
    "@types/cors": "^2.8.6",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.3",
    "@types/generic-pool": "^3.1.9",
    "@types/react-helmet": "^5.0.15",
    "@types/validator": "^12.0.1",
    "ajv": "^6.12.6",
    "autoprefix": "^1.0.1",
    "aws-sdk": "^2.712.0",
    "cherio": "^1.0.0-rc.2",
    "clipboard-copy": "^3.1.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
