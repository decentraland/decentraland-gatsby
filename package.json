{
  "name": "decentraland-gatsby",
  "version": "0.0.0-development",
  "description": "Common Gastby components",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "test": "jest",
    "build": "npm run build:clear && npm run build:code && npm run build:plugin:intl && npm run build:style",
    "build:clear": "rm -rf dist",
    "build:code": "tsc -p . -outDir dist --sourceMap false --skipLibCheck",
    "build:plugin:intl": "cp src/plugins/intl/package.json dist/plugins/intl",
    "build:style": "postcss --base src --dir dist 'src/**/*.css'",
    "start": "npm run storybook",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,mdx,css}'",
    "storybook": "npx storybook@7 dev -p 6006",
    "semantic-release": "semantic-release",
    "build-storybook": "npx storybook@7 build && cp -r ./static/* ./storybook-static && mv ./storybook-static ./public",
    "husky-setup": "husky install",
    "migrate": "DOTENV_CONFIG_PATH=.env.development ts-node -r dotenv/config ./node_modules/node-pg-migrate/bin/node-pg-migrate -j ts -m src/migrations -d CONNECTION_STRING",
    "precommit": "npm run todo:up && git add TODO.md && lint-staged --config .husky/pre-commit.js",
    "todo": "leasot '{src,docs}/**/*.{ts,tsx,css,md,yml}'",
    "todo:up": "leasot '{src,docs}/**/*.{ts,tsx,css,md,yml}' --reporter json | node bin/todo.js > TODO.md",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/decentraland/decentraland-gatsby"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "ci": false
  },
